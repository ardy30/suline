<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="boeren.com.boeren.app.bmedical.boeren.fragments.FeedbackFragment">

    <boeren.com.appsuline.app.bmedical.appsuline.font.DinProTextView
        android:id="@+id/title_feedback"
        android:text="@string/title_feedback"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/title_color"
        style="@style/TextViewAppearance.Display1"
        />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/title_feedback"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <boeren.com.appsuline.app.bmedical.appsuline.font.DinProTextView
            android:id="@+id/tv_feebackp1"
            android:text="@string/Text_feedback"
            android:textColorLink="@color/theme_color"
            style="@style/TextViewAppearance.Body1"
            />


    </LinearLayout>
    </ScrollView>
</RelativeLayout>
