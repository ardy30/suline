        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
    <boeren.com.appsuline.app.bmedical.appsuline.font.DinProTextView
        android:id="@+id/textview"
        android:text="@string/Text_Hyper"
        style="@style/TextViewAppearance.Body1"
        />
        <boeren.com.appsuline.app.bmedical.appsuline.font.DinProTextView
            android:text="@string/hyper_title_veelplassen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/theme_color"
            android:textStyle="bold"
            style="@style/TextViewAppearance.Headline2"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imview_hyper_veelplassen"
            android:src="@drawable/hyper_plassen"
            android:background="@drawable/imageview_border"
            android:layout_marginStart="@dimen/activity_horizontal_margin"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:layout_marginBottom="@dimen/activity_horizontal_margin"
            />
        <boeren.com.appsuline.app.bmedical.appsuline.font.DinProTextView
